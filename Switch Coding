package exercise;

public class Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method sub
				char ch='b';
				switch(ch) {
				case 'a':
					System.out.println("ch=a");
				case 'b':
					System.out.println("ch=b");
				case 'c':
					System.out.println("ch=c");
				default :
					System.out.println("default");
				}

	}

}
