package mySwitch;
import java.util.Scanner;
public class MyGradeSwitch2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter your marks : ");
		int marks = sc.nextInt();
		
		switch(marks/10) { //kena bahagi 10 untuk dapatkan real number untuk case
		
		case 10 :
		case 9 :
			System.out.println("Excellent : Grade A");
			break;
		
		case 8 :
			System.out.println("Very Good : Grade B");
			break;
		
		case 7 :
			
			System.out.println("Good : Grade C");
			break;
		
		case 6 :
			System.out.println("Good : Grade D");
			break;
		
		case 5 :
			System.out.println("Good Grade E");
			break;
		
		case 4 :
			System.out.println("Just passed : Grade F");
			break;
		
		default:
			System.out.println("Alamak, FAIL!!!");
			
			sc.close();
		}
	}

}
